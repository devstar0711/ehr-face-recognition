    "version": "0.2.0",
    "configurations": [
        {
            "name": "example Mobile (debug mode)",
            "cwd": "example",
            "request": "launch",
            "type": "dart"
        },
        {
            "name": "example Mobile (profile mode)",
            "cwd": "example",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile"
        },
        {
            "name": "example Mobile (release mode)",
            "cwd": "example",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release"
        },
        {
            "name": "example Desktop debug",
            "cwd": "example",
            "request": "launch",
            "type": "dart",
            "args": [],
            "program": "lib/main_desktop.dart"
        },
        {
            "name": "example Desktop Release",
            "cwd": "example",
            "request": "launch",
            "type": "dart",
            "args": ["--release"],
            "program": "lib/main_desktop.dart"
        },
        {
            "name": "Debug native Linux",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/example/build/linux/x64/debug/bundle/flutter_opencv_dlib_example",
            "cwd": "${workspaceFolder}"
        }
    ]
}